{
  "id": "io.github.sepehr_rs.LibreSudoku.Devel",
  "runtime": "org.gnome.Platform",
  "runtime-version": "45",
  "sdk": "org.gnome.Sdk",
  "command": "libresudoku",
  "finish-args": [
    "--share=network",
    "--share=ipc",
    "--socket=fallback-x11",
    "--socket=wayland",
    "--device=dri",
    "--filesystem=home",
    "--own-name=io.github.sepehr_rs.LibreSudoku"
  ],
  "modules": [
    "python3-requirements.json",
    {
      "name": "libresudoku",
      "buildsystem": "meson",
      "sources": [
        {
          "type": "dir",
          "path": "."
        }
      ]
    }
  ]
}
